    "fs-path": "^0.0.25",
    "i2c-bus": "^5.2.2",
    "js-yaml": "^3.13.1",
    "md5": "^2.2.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.40.0",
    "moment": "^2.29.2",
    "multer": "^1.4.4",
